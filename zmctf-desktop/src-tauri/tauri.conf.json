{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "ZMctf",
  "version": "0.1.0",
  "identifier": "com.zmctf.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": "default-src 'self'; connect-src 'self' http://localhost:* http://127.0.0.1:*; img-src 'self' data: blob:; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-eval';"
    },
    "windows": [
      {
        "title": "ZMctf",
        "width": 1280,
        "height": 840,
        "minWidth": 1024,
        "minHeight": 680,
        "resizable": true
      }
    ]
  },
  "bundle": {},
  "plugins": {}
}
